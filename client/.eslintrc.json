{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "globals": {
        "add_confirm": true,
        "delete_search_button": true,
        "delete_confirm": true,
        "edit_search_button": true,
        "edit_confirm": true,
        "toggle_all": true,
        "submit_map": true,
        "submit_country": true
    }
}